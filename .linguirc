{
  "locales": [
    "en",
    "fr",
    "es",
    "ja",
    "pseudo-LOCALE"
  ],
  "catalogs": [
    {
      "path": "public/locales/{locale}/messages",
      "include": [
        "public/app"
      ],
      "exclude": [
        "**/*.d.ts",
        "**/*.test.ts",
        "**/node_modules/**",
        "public/app/plugins"
      ]
    }
  ],
  "fallbackLocales": {
    "pseudo-LOCALE": "ja",
    "default": "ja"
  },
  "pseudoLocale": "pseudo-LOCALE",
  "sourceLocale": "ja",
  "format": "po",
  "formatOptions": {
    "lineNumbers": false
  }
}
